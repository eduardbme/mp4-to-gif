{
  "version": "VERSION",
  "env": "ENV",
  "httpApiServer": {
    "host": "HTTP_API_SERVER_HOST",
    "port": {
      "__name": "HTTP_API_SERVER_PORT",
      "__format": "number"
    },
    "origins": {
      "__name": "HTTP_API_SERVER_ORIGINS",
      "__format": "json"
    }
  },
  "redis": "REDIS_URL",
  "logger": {
    "level": "LOGGER_LEVEL"
  },
  "document": {
    "concurrency": {
      "__name": "DOCUMENT_CONCURRENCY",
      "__format": "number"
    },
    "repository": {
