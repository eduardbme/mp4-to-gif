{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "typeRoots": ["node_modules/@types", "types"],
    "types": ["@branded"],
    "paths": {
      "@common/domain": ["libs/common/domain/src/index.ts"],
      "@common/util": ["libs/common/util/src/index.ts"],
      "@common/validate": ["libs/common/validate/src/index.ts"],
      "@mp4-to-gif-api/common/context": [
        "libs/mp4-to-gif-api/common/context/src/index.ts"
      ],
      "@mp4-to-gif-api/common/domain": [
        "libs/mp4-to-gif-api/common/domain/src/index.ts"
      ],
      "@mp4-to-gif-api/common/injector": [
        "libs/mp4-to-gif-api/common/injector/src/index.ts"
      ],
      "@mp4-to-gif-api/common/logger": [
        "libs/mp4-to-gif-api/common/logger/src/index.ts"
      ],
      "@mp4-to-gif-api/document/adapter": [
        "libs/mp4-to-gif-api/document/adapter/src/index.ts"
      ],
      "@mp4-to-gif-api/document/domain": [
        "libs/mp4-to-gif-api/document/domain/src/index.ts"
      ],
      "@mp4-to-gif/api/data-access": [
        "libs/mp4-to-gif/api/data-access/src/index.ts"
      ],
      "@mp4-to-gif/api/domain": ["libs/mp4-to-gif/api/domain/src/index.ts"],
      "@mp4-to-gif/document/data-access": [
        "libs/mp4-to-gif/document/data-access/src/index.ts"
      ],
      "@mp4-to-gif/document/domain": [
        "libs/mp4-to-gif/document/domain/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
